{
  "name": "vueless",
  "version": "1.1.1-beta.25",
  "description": "Vue Styleless UI Component Library, powered by Tailwind CSS.",
  "author": "Johnny Grid <hello@vueless.com> (https://vueless.com)",
  "homepage": "https://vueless.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "pre:start": "npx node .scripts/icons",
    "dev:docs": "storybook dev -p 6006 --docs --no-open",
    "dev": "storybook dev -p 6006 --no-open",
    "build": "storybook build --docs",
    "preview": "vite preview --host --outDir=storybook-static",
    "ts:check": "vue-tsc --build --force",
    "release:prepare": "npm run pre:start && rm -rf dist && mkdir -p dist && cp -r src/. package.json LICENSE README.md dist/ && npx node .scripts/writeLocales",
    "release:beta": "release-it --ci --npm.publish --preRelease=beta --increment=prerelease",
    "release:patch": "release-it patch --ci --npm.publish --git.tag --github.release",
    "release:minor": "release-it minor --ci --npm.publish --git.tag --github.release",
    "release:major": "release-it major --ci --npm.publish --git.tag --github.release",
    "lint": "eslint --no-fix src/ .storybook/",
    "lint:fix": "eslint --fix src/ .storybook/",
    "lint:ci": "eslint --no-fix --max-warnings=0",
    "test": "vitest",
    "test:ci": "vitest --run"
  },
  "bin": {
    "vueless": "./bin/index.js"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "cva": "^1.0.0-beta.4",
    "lodash-es": "^4.17.21",
    "svgo": "^4.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tippy.js": "^6.3.7",
    "unplugin-vue-components": "^28.8.0",
    "vuedraggable": "4.1.0"
  },
  "devDependencies": {
    "@material-symbols/svg-500": "^0.33.0",
    "@release-it/bumper": "^7.0.5",
    "@tsconfig/node20": "^20.1.6",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "@vueless/storybook": "^1.1.3",
    "eslint": "^9.32.0",
    "eslint-plugin-storybook": "^9.0.18",
    "eslint-plugin-vue": "^10.3.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "release-it": "^19.0.4",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vitest": "^3.2.4",
    "vue": "^3.5.18",
    "vue-i18n": "^11.1.11",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.4"
  },
  "style": "tailwind.css",
  "types": "index.d.ts",
  "main": "index.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.ts",
      "types": "./types.ts",
      "style": "./tailwind.css"
    },
    "./composables/*": {
      "import": "./composables/*.ts"
    },
    "./utils/*": {
      "import": "./utils/*.ts"
    },
    "./utils/node/*": {
      "import": "./utils/node/*.js"
    },
    "./constants": {
      "import": "./constants.js"
    },
    "./plugin-vite": {
      "import": "./plugin-vite.js",
      "types": "./plugin-vite.d.ts"
    },
    "./ui.*": {
      "import": "./ui.*"
    },
    "./directives/*": {
      "import": "./directives/*"
    },
    "./modules": {
      "types": "./modules.d.ts"
    }
  },
  "resolutions": {
    "jackspeak": "2.3.6"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuelessjs/vueless.git"
  },
  "bugs": {
    "url": "https://github.com/vuelessjs/vueless/issues"
  },
  "keywords": [
    "vueless",
    "vue",
    "vue.js",
    "vue3",
    "ui library",
    "component library",
    "vue framework",
    "design system",
    "tailwind",
    "tailwindcss",
    "unstyled",
    "styleless",
    "headlessui",
    "ui"
  ]
}
