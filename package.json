{
  "name": "vueless",
  "version": "0.0.51",
  "license": "MIT",
  "description": "Vue Styleless Component Framework.",
  "homepage": "https://vueless.com",
  "author": "Johnny Grid",
  "main": "index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --ext .vue,.js,.ts --no-fix --ignore-path .eslintignore src/ .storybook/ tests/",
    "lint:fix": "eslint --ext .vue,.js,.ts --fix --ignore-path .eslintignore src/ .storybook/ tests/",
    "lint:ci": "eslint --ext .vue,.js,.ts --no-fix --ignore-path .eslintignore --max-warnings=0",
    "release:patch": "release-it patch --ci --npm.publish",
    "release:minor": "release-it minor --ci --npm.publish --git.tag --github.release",
    "release:major": "release-it major --ci --npm.publish --git.tag --github.release",
    "sb:dev-full": "STORYBOOK_VUELESS_ENV=1 STORYBOOK_FULL=1 storybook dev -p 6006 --no-open",
    "sb:dev": "storybook dev -p 6006 --docs --no-open",
    "sb:build": "STORYBOOK_VUELESS_ENV=1 storybook build --docs",
    "sb:preview": "vite preview --host --outDir=storybook-static",
    "package:prepare": "node src/gen.web-types && rm -rf dist && mkdir -p dist && cp -r src/. package.json LICENSE web-types.json README.md dist/ && rm -rf dist/assets && node dist.prepare"
  },
  "dependencies": {
    "@material-symbols/svg-500": "^0.14.3",
    "@tailwindcss/forms": "^0.5.4",
    "@uppy/core": "^3.3.1",
    "@uppy/drag-drop": "^3.0.2",
    "@uppy/vue": "^1.0.2",
    "cva": "^1.0.0-beta.1",
    "lodash-es": "^4.17.21",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "vue-tippy": "^6.0.0-beta.0",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@release-it/bumper": "^6.0.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vueless/plugin-vite": "^0.0.1",
    "@vueless/storybook": "^0.0.8",
    "autoprefixer": "^10.4.19",
    "cssnano": "^6.1.2",
    "eslint": "^8.55.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "eslint-plugin-vue": "^9.25.0",
    "globby": "^13.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "release-it": "^17.2.1",
    "vite": "^5.2.10",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vue": "^3.4.25"
  },
  "resolutions": {
    "jackspeak": "2.3.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuelessjs/vueless.git"
  },
  "bugs": {
    "url": "https://github.com/vuelessjs/vueless/issues"
  },
  "web-types": "./web-types.json"
}
